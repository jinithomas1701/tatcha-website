<div class="sub-details-total">
    <div class="total-section">
        <div class="content-section">                    
            <div class="total-header">
                <h3 class="order-section-title"> ${Resource.msg('club.vieworder.totals', 'sor_smartorderrefill', null)}</h3>
            </div>
            <div class="total-info">
                <p><span>${Resource.msg('club.vieworder.subtotal', 'sor_smartorderrefill', null)}</span><span><isprint value="${new dw.value.Money(total, pdict.currencyCode)}" /></span></p>
                <p><span>${Resource.msg('club.vieworder.shipping', 'sor_smartorderrefill', null)} (${pdict.ShippingMethodName})</span><span><isprint value="${new dw.value.Money(pdict.ShippingCost, pdict.currencyCode)}" /></span></p>
                <p><span>${Resource.msg('club.vieworder.ordertotal', 'sor_smartorderrefill', null)}</span><span><isprint value="${new dw.value.Money((total+pdict.ShippingCost), pdict.currencyCode)}" /></span></p>
            </div>
        </div>
    </div>
</div>