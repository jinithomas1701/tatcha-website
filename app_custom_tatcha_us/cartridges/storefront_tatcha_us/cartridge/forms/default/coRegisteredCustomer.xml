<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19" secure="false">

	<field
		formid="email"
		label="label.input.email.profile"
		mandatory="true"
		max-length="50"
		missing-error="validate.email"
		parse-error="validate.email"
		range-error="error.message.50orless"
		regexp="^[\w.%+-]+@[\w.-]+\.[\w]{2,6}$"
		value-error="ValueErrorText"
		type="string"/>

	<field
		formid="password"
		label="label.input.password.profile"
		mandatory="true"
		missing-error="validate.password"
		type="string" />

</form>
